<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/x_600"
    android:layout_height="@dimen/y_360"
    xmlns:skin="http://schemas.android.com/android/skin"
    android:background="@drawable/white_frame"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/connect_wifi_name"
        android:textColor="@color/black"
        android:textSize="@dimen/x_26"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y_30"
        />
    
    <TextView
        android:layout_width="@dimen/x_337"
        android:layout_height="wrap_content"
        android:id="@+id/et_password"
        android:layout_below="@id/connect_wifi_name"
        android:layout_marginTop="@dimen/y_30"
        android:layout_centerHorizontal="true"
        android:text="@string/isconnect"
        android:paddingStart="@dimen/x_10"
        android:paddingEnd="@dimen/x_10"
        android:textColor="@color/black"
        android:textSize="@dimen/x_21"
        
        />
    

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/y_60"
        android:id="@+id/enter"
        android:layout_alignParentBottom="true"
        android:gravity="center"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:background="@drawable/button_bg"
        android:textColor="@color/white"
        android:text="@string/enter"
        android:textSize="@dimen/x_25"
        android:layout_marginStart="@dimen/x_30"
        android:layout_marginBottom="@dimen/y_30"
        android:minWidth="@dimen/x_150"
        android:paddingStart="@dimen/x_5"
        android:paddingEnd="@dimen/x_5"
        />
    
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/y_60"
        android:id="@+id/cancel"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:gravity="center"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:background="@drawable/button_bg"
        android:textColor="@color/white"
        android:text="@string/cancel"
        android:textSize="@dimen/x_25"
        android:layout_marginEnd="@dimen/x_30"
        android:layout_marginBottom="@dimen/y_30"
        android:minWidth="@dimen/x_150"
        android:paddingStart="@dimen/x_5"
        android:paddingEnd="@dimen/x_5"
        />

</RelativeLayout>